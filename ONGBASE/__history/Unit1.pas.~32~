{$R *.dfm}



procedure TForm1.ActiverPNNC1Click(Sender: TObject);
var
i:string;
begin
PageControl1.Visible:=true;
A.PageControl.Enabled:=False;
S.PageControl.Enabled:=True;
p.PageControl.Enabled:=false;
tabSheet1.PageControl.Enabled:=false;
TabSheet2.PageControl.Enabled:=false;
end;

procedure TForm1.FormCreate(Sender: TObject);
begin
PageControl1.Visible:=false;
end;
end.
